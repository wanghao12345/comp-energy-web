{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-layout": "^6.5.0",
    "@types/redux-logger": "^3.0.9",
    "antd": "^4.19.2",
    "echarts": "^5.3.1",
    "immer": "9.0.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "17.x",
    "react-dom": "17.x",
    "redux-logger": "^3.0.6",
    "styled-components": "^5.3.3",
    "umi": "^3.5.21",
    "use-immer": "0.6.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.180",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.24",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.21",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
